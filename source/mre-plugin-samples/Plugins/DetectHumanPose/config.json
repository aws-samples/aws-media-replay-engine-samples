{
  "Lambda": {
    "Handler": "DetectHumanPose.lambda_handler",
    "MemorySize": 512,
    "TimeoutSecs": 90,
    "IAMPolicyDocument": [
      {
        "Actions": [
          "sagemaker:InvokeEndpoint"
        ],
        "Resources": [
          "*"
        ]
      }
    ],
    "Layers": [
      "scipy",
      "opencv-python"
    ]
  },
  "MRE": {
    "Plugin": {
      "Name": "DetectHumanPose",
      "Description": "This plugin classify human pose keypoints into predefined classes.",
      "Class": "Featurer",
      "ExecutionType": "SyncModel",
      "SupportedMediaType": "Video",
      "ContentGroups": [
        "All"
      ],
      "ExecuteLambdaQualifiedARN": "",
      "ModelEndpoints": [
        {
          "Name": "PoseClassificationModel"
        }
      ],
      "Configuration": {
        "minimum_confidence": "0.7",
        "Keypoints_List": [0,1,2,3]
      },
      "OutputAttributes": {
        "Label": {
          "Description": ""
        },
        "PoseDetection": {
          "Description": ""
        }
      },
      "DependentPlugins": [
        "DetectPoseKeypoints"
      ]
    }
  }
}
